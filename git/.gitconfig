[credential]
        helper = 
        helper = /usr/local/share/gcm-core/git-credential-manager
[credential "https://dev.azure.com"]
        useHttpPath = true
[alias]
        tree = log --graph --pretty=format:'%x09%C(auto) %h %Cgreen %ar %Creset%x09by"%C(cyan ul)%an%Creset" %x09%C(auto)%s %d'
        sw = switch
        bd = branch -d
        po = push origin
        camd = commit --amend
        bcur = branch --contains
        rhh = reset HEAD --hard
        rp = pull -r origin
        rc = rebase --continue
        delete-merged-branch = "!f () { git checkout $1; git branch --merged|egrep -v '\\*|develop|master|main'|xargs git branch -d; };f"